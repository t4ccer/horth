#include elf64_syscalls

proc str_len (ptr -> int)
  proc str_len_go (ptr int -> ptr int)
    dup read1 0 eqi not if
      1 add
      swap
      1 add
      swap
      str_len_go
    end
  end
  0 swap str_len_go pop
end

proc print_null_term_str (ptr -> )
  dup str_len swap 1 sys_write pop
end

proc print_bool (bool -> )
  dup if
    "true" print_null_term_str
  end
  not if
    "false" print_null_term_str
  end
end

proc neqi (int int -> bool)
  eqi not
end
