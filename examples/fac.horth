#include stdlib

proc fac (int -> int)
  proc fac_go (int int -> int int)
    dup 0 eqi not
    if
      swap over mul swap
      1 sub
      fac_go
    end
  end
  1 swap fac_go pop
end

proc square (int -> int)
  dup mul
end

3 square fac dup printi
"Is (3^2)! == 362880: " print_str 362880 eqi print_bool
